{
  "fmt": {
    "files": {
      "exclude": [".git", ".deno", "cov"]
    }
  },
  "lint": {
    "files": {
      "exclude": [".git", ".deno", "cov"]
    }
  },
  "tasks": {
    "cache": "deno cache -r --lock=lock.json --lock-write --no-check mod.ts",
    "check": "deno check mod.ts",
    "test": "deno test -A --unstable --coverage=./cov"
  }
}
